    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^30.2.0",
    "multer": "^2.0.2",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
